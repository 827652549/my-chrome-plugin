{
	"name":"苏一恒的自定义插件",
	"version":"1.0",
	"manifest_version":2,
	"content_scripts":[
		{
			"matches":["<all_urls>"],
			"js":["index.js"]
		}
	],
	"browser_action":{
		"default_icon":"icon.png",
		"default_popup":"popup.html"
	},
	"icons":{
		"16":"icon.png",
		"32":"icon.png",
		"128":"icon.png"
	}

}